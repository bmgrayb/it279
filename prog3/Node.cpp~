 * This represents one node in the priority queue
 */

#include "Node.h"
using namespace std;

template<class T>
Node::Node(T data, float priority)
{
  this->data = data;
  this-> priority = priority;
}

template<class T>
T Node::getData()
{
  return data;
}

template<class T>
float Node::getPriority()
{
  return priority;
}

template<class T>
void Node::setPriority(float newPriority)
{
  priority = newPriority;
}
